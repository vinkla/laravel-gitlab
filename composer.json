{
    "name": "vinkla/gitlab",
    "description": "A GitLab bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "gitlab", "git", "api"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.*|5.1.*",
        "illuminate/support": "5.0.*|5.1.*",
        "graham-campbell/manager": "~2.1",
        "m4tthumphrey/php-gitlab-api": "~7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\GitLab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\GitLab\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
